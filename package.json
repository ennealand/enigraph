{
  "name": "enigraph",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode lib && tsc --p ./tsconfig.build.json && echo \"import './style.css';$(cat ./dist/index.js)\" > ./dist/index.js",
    "build:app": "tsc && vite build",
    "preview": "vite preview"
  },
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "preact": "./dist/source/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts",
      "preact": "./dist/source/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@preact/preset-vite": "^2.8.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.11.21",
    "d3": "^7.8.5",
    "million": "^3.0.6",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-typed-css-modules": "^0.1.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "peerDependencies": {
    "@modular-forms/preact": "^0.7.0",
    "@preact/signals": "^1.2.2",
    "@preact/signals-core": "^1.5.1",
    "preact": "^10.19.6"
  },
  "dependencies": {
    "@million/lint": "latest",
    "deepsignal": "^1.5.0",
    "preact": "8"
  }
}
